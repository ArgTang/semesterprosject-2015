<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.WelcomeController">
   <right>
      <GridPane maxHeight="-Infinity" vgap="5.0" BorderPane.alignment="CENTER_LEFT">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="160.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="-Infinity" minHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="-Infinity" minHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="-Infinity" minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
             <Label text="Brukernavn" />
             <TextField fx:id="userName" promptText="brukernavn" GridPane.columnIndex="1" />
             <Label text="Passord" GridPane.rowIndex="1" />
             <PasswordField fx:id="password" promptText="passord" GridPane.columnIndex="1" GridPane.rowIndex="1" />
             <Button fx:id="loginButton" onAction="#login" text="Login" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </right>
   <center>
      <TilePane alignment="CENTER_LEFT">
         <children>
            <Label alignment="TOP_RIGHT" contentDisplay="CENTER" prefHeight="66.0" prefWidth="336.0" text="Velkommen til Oss!" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </TilePane>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
