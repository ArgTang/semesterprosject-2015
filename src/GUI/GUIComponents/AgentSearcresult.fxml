<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane hgap="10.0" maxHeight="200.0" maxWidth="500.0" minHeight="220.0" minWidth="220.0" HBox.hgrow="ALWAYS">
         <children>
            <Label text="Personnummer" />
            <Label fx:id="socialsecurity" GridPane.columnIndex="1" />
            <Label text="Kundenummer" GridPane.rowIndex="1" />
            <Label fx:id="customerId" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Navn" GridPane.rowIndex="2" />
            <Label fx:id="fullname" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Adresse" GridPane.rowIndex="4" />
            <Label fx:id="Adress" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="Telefonnummer" GridPane.rowIndex="3" />
            <Label fx:id="Phonenumber" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="Poststed" GridPane.rowIndex="5" />
            <Label fx:id="City" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Button fx:id="editPerson" maxHeight="1.7976931348623157E308" mnemonicParsing="false" text="Endre Kundeinformasjon" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="6" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="NEVER" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
         </rowConstraints>
         <padding>
            <Insets left="10.0" top="10.0" />
         </padding>
      </GridPane>
      <VBox maxHeight="1.7976931348623157E308" HBox.hgrow="ALWAYS">
         <children>
            <Label text="Registrerte saker:" />
            <TableView fx:id="displayRegisteredInsuranceIncident" maxHeight="1.7976931348623157E308" minWidth="250.0">
               <columns>
                  <TableColumn editable="false" prefWidth="-1.0" text="Type" />
                  <TableColumn editable="false" prefWidth="-1.0" text="Year" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</HBox>
