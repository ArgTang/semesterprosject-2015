<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane hgap="10.0" maxHeight="200.0" maxWidth="400.0" minHeight="200.0" minWidth="200.0">
         <children>
            <Label text="PersonNummer" />
            <Label text="Kundenummer" GridPane.rowIndex="1" />
            <Label text="Navn" GridPane.rowIndex="2" />
            <Label text="Adresse" GridPane.rowIndex="4" />
            <Label text="Telefonnummer" GridPane.rowIndex="3" />
            <Label text="Poststed" GridPane.rowIndex="5" />
            <Label fx:id="socialsecurity" GridPane.columnIndex="1" />
            <Label fx:id="customerId" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="fullname" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="editPerson" mnemonicParsing="false" text="Rediger" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="6" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
         </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
      <TableView fx:id="displayRegisteredInsuranceIncident" prefHeight="440.0" prefWidth="260.0">
        <columns>
          <TableColumn prefWidth="104.0" text="Type" />
          <TableColumn prefWidth="90.0" text="Year" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</HBox>
